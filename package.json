{
  "name": "generator-elm",
  "version": "0.0.31",
  "description": "Generate a minimal Elm + Webpack project",
  "repository": "danneu/generator-elm",
  "author": "Dan Neumann <danrodneu@gmail.com> (https://github.com/danneu)",
  "scripts": {
    "test": "node_modules/.bin/mocha",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags",
    "deploy": "git pull --rebase origin master && git push origin master",
    "pretty-fix": "./node_modules/.bin/prettier --write \"{generators,test}/**/*.js\""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "elm-lang",
    "elm",
    "generator",
    "webpack",
    "boilerplate"
  ],
  "dependencies": {
    "chalk": "^2.1.0",
    "yeoman-generator": "^3.1.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "license": "MIT",
  "devDependencies": {
    "mocha": "^4.0.1",
    "prettier": "^1.7.4",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^2.0.0"
  }
}
